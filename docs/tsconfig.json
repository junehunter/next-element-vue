{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "lib": [
            "WebWorker"
        ],
        "paths": {
            "~/*": [
                "./.vitepress/vitepress/*"
            ]
        },
        "types": [
            "vite/client",
            "node",
        ],
        "jsx": "preserve"
    },
    "include": [
        "**/*",
        ".vitepress/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}