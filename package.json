		"@tensorflow/tfjs": "^4.11.0",
		"@types/lodash-es": "^4.17.9",
		"@vueuse/core": "^10.4.1",
		"element-plus": "^2.8.0",
		"flv.js": "^1.6.2",
		"lodash-es": "^4.17.21",
		"mpegts.js": "^1.7.3",
		"video.js": "^8.6.0",
		"vue": "^3.4.21",
		"vue-i18n": "^9.4.1",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@babel/core": "^7.22.20",
		"@babel/plugin-transform-runtime": "^7.22.15",
		"@babel/preset-env": "^7.22.20",
