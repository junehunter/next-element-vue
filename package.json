		"@types/node": "^20.6.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"@vue/babel-plugin-jsx": "^1.1.5",
		"@vue/compiler-sfc": "^3.3.4",
		"@vueuse/core": "^10.4.1",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.1",
		"cssnano": "^6.0.1",
		"element-plus": "^2.8.0",
		"eslint": "^8.50.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-vue": "^9.17.0",
		"flv.js": "^1.6.2",
		"lint-staged": "^14.0.1",
		"lodash-es": "^4.17.21",
		"mockjs": "^1.1.0",
		"mpegts.js": "^1.7.3",
		"postcss-import": "^15.1.0",
		"postcss-scss": "^4.0.8",
		"prettier": "^3.0.3",
		"rimraf": "^5.0.1",
