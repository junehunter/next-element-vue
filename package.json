		"unplugin-vue-macros": "^2.5.1",
		"video.js": "^8.6.0",
		"vitest": "^0.34.5",
		"vue": "^3.4.21",
		"vue-eslint-parser": "^9.3.1",
		"vue-i18n": "^9.4.1",
		"vue-router": "^4.2.5"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*.{vue,js,jsx,json}": ["eslint --fix"],
		"*.ts?(x)": ["eslint --fix"]
	},
	"engines": {
		"node": ">=16"
	},
	"license": "MIT"
}

